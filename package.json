{
  "name": "romap",
  "version": "0.0.1",
  "description": "OpenLayers with React",
  "main": "lib/index.js",
  "typings": "lib/index.d.ts",
  "module": "lib/index.js",
  "files": [
    "lib",
    "src"
  ],
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "webpack-dev-server --host 0.0.0.0 --port 3000 --config webpack.dev.config.js --content-base src --hot --inline",
    "prettier": "prettier --single-quote --print-width 120 --write --parser typescript '{src, test}/**/*.ts'; prettier --single-quote --print-width 120 --write --parser typescript '{src,test}/**/*.tsx'",
    "lint": "npm run prettier && tslint '{src, test}/**/.{ts,tsx}'",
    "build": "rimraf lib && npm run lint && tsc -p tsconfig.json",
    "precommit": "npm run build && git add"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/aptogeo/romap.git"
  },
  "keywords": [
    "Web mapping",
    "React",
    "OpenLayers"
  ],
  "author": "Mathieu MAST",
  "license": "ISC",
  "devDependencies": {
    "@babel/preset-env": "^7.1.5",
    "@types/lodash": "^4.14.118",
    "@types/openlayers": "^4.6.12",
    "@types/prop-types": "^15.5.6",
    "@types/react": "^16.7.3",
    "@types/react-dom": "^16.0.9",
    "awesome-typescript-loader": "^5.2.1",
    "babel-core": "^6.26.3",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.4",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "css-loader": "^1.0.1",
    "html-webpack-plugin": "^3.2.0",
    "ol": "5.3.0",
    "prettier": "^1.15.2",
    "proj4": "2.5.0",
    "rimraf": "^2.6.2",
    "source-map-loader": "^0.2.4",
    "style-loader": "^0.23.1",
    "tslint": "^5.11.0",
    "tslint-config-prettier": "^1.15.0",
    "tslint-react": "^3.6.0",
    "typescript": "3.1.6",
    "webpack": "^4.25.1",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "^3.1.10"
  },
  "dependencies": {
    "es6-promise": "^4.2.5",
    "lodash": "^4.17.11",
    "prop-types": "^15.6.2",
    "react": "^16.6.1",
    "react-dom": "^16.6.1"
  },
  "peerDependencies": {
    "ol": "5.3.0",
    "proj4": "2.5.0"
  }
